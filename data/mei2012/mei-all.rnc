           "          "
           sch:assert [
             test =
               "@startid or @tstamp or @tstamp.ges or                 @tstamp.real"
             "\x{a}" ~
             "            Must have one of the attributes: startid, tstamp, tstamp.ges or tstamp.real"
           ]
           "\x{a}" ~
           "          "
           sch:assert [
             test = "@endid or @dur"
