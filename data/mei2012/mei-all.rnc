    empty
    >> sch:pattern [
         id =
           "slur-constraint-Start-_and_end-type_attributes_required_on_some_control_events"
         "\x{a}" ~
         "        "
         sch:rule [
           context = "mei:slur"
           "\x{a}" ~
           "          "
           sch:assert [
             test =
               "@startid or @tstamp or @tstamp.ges or                 @tstamp.real"
             "\x{a}" ~
